{
  "name": "npm-audit-html",
  "version": "0.0.0-development",
  "description": "Generate a HTML report for NPM Audit",
  "main": "index.js",
  "bin": {
    "npm-audit-html": "index.js"
  },
  "scripts": {
    "test": "npm run lint && npm run audit-report && npm audit",
    "lint": "eslint '*.js' 'lib/**/*.js'",
    "lint:fix": "eslint '*.js' 'lib/**/*.js' --fix",
    "lint!": "npm run format && npm run lint:fix",
    "format": "prettier --write '*.js' 'lib/**/*.js'",
    "audit-report": "npm audit --json | node index.js -o tests/npm-audit.html",
    "cm": "git-cz",
    "pre-commit": "lint-staged",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once --pro"
  },
  "author": "eventOne Labs <opensource@event1.io>",
  "repository": "eventOneHQ/npm-audit-html",
  "license": "MIT",
  "devDependencies": {
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "semantic-release": "^15.14.0",
    "travis-deploy-once": "^5.0.11"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-material-design": "^4.1.1",
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "datatables": "^1.10.18",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.7.6",
    "highlight.js": "^9.16.2",
    "jquery": "^3.3.1",
    "marked": "^0.7.0",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "terminal-link": "^1.3.0",
    "update-notifier": "^3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint!",
      "git add"
    ]
  }
}
